{
  "name": "goldenspiral-modules",
  "authors": [
    "Alex Leach <alex@goldenspiralmarketing.com>",
    "Kyle Numann <kyle@goldenspiralmarketing.com>",
    "Andy Wells <andy@goldenspiralmarketing.com>"
  ],
  "dependencies": {
    "bootstrap-sass": "bootstrap-sass-official#~3.3.5",
    "jquery-validation": "^1.15.0",
    "gsap": "^1.19.1",
    "magnific-popup": "^1.1.0",
    "js-cookie": "^2.1.3",
    "sticky-kit": "^1.1.2",
    "slick-carousel": "^1.6.0",
    "imagesloaded": "^4.1.3",
    "barba.js": "luruke/barba.js#^1.0.0",
    "svg4everybody": "^2.1.9",
    "requirejs": "^2.3.5",
    "bind.js": "^1.1.1",
    "bootstrap": "https://github.com/twbs/bootstrap.git#4.3.1"
  },
  "install": {
    "options": {
      "includePackageNameInInstallPath": false
    },
    "ignore": [
      "jquery"
    ],
    "base": "_lib",
    "path": {
      "js": "js/{name}",
      "css": "css/{name}",
      "scss": "scss/{name}",
      "less": "less/{name}",
      "eot": "fonts",
      "svg": "fonts",
      "ttf": "fonts",
      "woff": "fonts",
      "woff2": "fonts",
      "gif": "img",
      "png": "img",
      "jpg": "img",
      "jpeg": "img"
    },
    "sources": {
      "imagesloaded": "bower_components/imagesloaded/imagesloaded.pkgd.js",
      "bootstrap-sass": [
        "bower_components/bootstrap-sass/assets/stylesheets/bootstrap/**",
        "bower_components/bootstrap-sass/assets/fonts/bootstrap/**"
      ],
      "bootstrap": [
        "bower_components/bootstrap/scss/**",
        "bower_components/bootstrap/js/src/**"
      ],
      "slick-carousel": [
        "bower_components/slick-carousel/slick/slick-theme.scss",
        "bower_components/slick-carousel/slick/slick.scss",
        "bower_components/slick-carousel/slick/slick.js",
        "bower_components/slick-carousel/slick/ajax-loader.gif",
        "bower_components/slick-carousel/slick/fonts/*"
      ],
      "magnific-popup": [
        "bower_components/magnific-popup/dist/jquery.magnific-popup.js",
        "bower_components/magnific-popup/src/css/*.scss"
      ],
      "barba.js": "bower_components/barba.js/dist/barba.js",
      "bind.js": "bower_components/bind.js/lib/bind.js"
    }
  }
}
